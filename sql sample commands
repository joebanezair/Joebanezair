use joebanezair;

select * from tbl_employee;

drop table tbl_employee;

create table list_of_names(ID BIGINT PRIMARY KEY NOT NULL, name varchar(50) NOT NULL);

insert into list_of_names(ID, name) values (02000123854, 'Joebanezair');

DELETE FROM tbl_employee WHERE EMPLOYEE_NO = 2000123854;

select * from list_of_names;

create table TBL_EMPLOYEE(
EMPLOYEE_NO BIGINT PRIMARY KEY NOT NULL,
FIRST_NAME varchar(50) NOT NULL,
LAST_NAME varchar(50) NOT NULL,
GENDER varchar(50) NOT NULL
);

insert into TBL_EMPLOYEE(
EMPLOYEE_NO,
FIRST_NAME, 
LAST_NAME, 
GENDER 
) VALUES (
2000123854,
'JOEBANEZAIR',
'BUATONA',
'MALE' 
);

create table TBL_EMPLOYEE(
EMPLOYEE_NO BIGINT PRIMARY KEY NOT NULL,
FIRST_NAME varchar(50) NOT NULL,
LAST_NAME varchar(50) NOT NULL,
GENDER varchar(50) NOT NULL,
BIRTH_DATE date NOT NULL,
HIRE_DATE date NOT NULL
);

insert into TBL_EMPLOYEE(
